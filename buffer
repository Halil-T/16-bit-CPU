#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x7fffebf9b030 .scope module, "bufftb" "bufftb" 2 1;
 .timescale 0 0;
v0x7fffebfe4c60_0 .var "A", 7 0;
v0x7fffebfe4d40_0 .var "B", 7 0;
RS_0x7f7f6be900a8 .resolv tri, v0x7fffebfe4490_0, v0x7fffebfe4ae0_0;
v0x7fffebfe4e10_0 .net8 "bus", 7 0, RS_0x7f7f6be900a8;  2 drivers
v0x7fffebfe4f30_0 .var "clk", 0 0;
v0x7fffebfe5020_0 .var "en_a", 0 0;
v0x7fffebfe5110_0 .var "en_b", 0 0;
S_0x7fffebf9b1b0 .scope module, "bA" "buffer" 2 7, 3 1 0, S_0x7fffebf9b030;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "in"
    .port_info 1 /INPUT 1 "en"
    .port_info 2 /INPUT 1 "clk"
    .port_info 3 /OUTPUT 8 "out"
v0x7fffebfae750_0 .net "clk", 0 0, v0x7fffebfe4f30_0;  1 drivers
v0x7fffebfe42e0_0 .net "en", 0 0, v0x7fffebfe5020_0;  1 drivers
v0x7fffebfe43a0_0 .net "in", 7 0, v0x7fffebfe4c60_0;  1 drivers
v0x7fffebfe4490_0 .var "out", 7 0;
E_0x7fffebfd3c20 .event posedge, v0x7fffebfae750_0;
S_0x7fffebfe4620 .scope module, "bB" "buffer" 2 8, 3 1 0, S_0x7fffebf9b030;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "in"
    .port_info 1 /INPUT 1 "en"
    .port_info 2 /INPUT 1 "clk"
    .port_info 3 /OUTPUT 8 "out"
v0x7fffebfe4880_0 .net "clk", 0 0, v0x7fffebfe4f30_0;  alias, 1 drivers
v0x7fffebfe4950_0 .net "en", 0 0, v0x7fffebfe5110_0;  1 drivers
v0x7fffebfe49f0_0 .net "in", 7 0, v0x7fffebfe4d40_0;  1 drivers
v0x7fffebfe4ae0_0 .var "out", 7 0;
    .scope S_0x7fffebf9b1b0;
T_0 ;
    %wait E_0x7fffebfd3c20;
    %load/vec4 v0x7fffebfe42e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x7fffebfe43a0_0;
    %assign/vec4 v0x7fffebfe4490_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 255, 8;
    %assign/vec4 v0x7fffebfe4490_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fffebfe4620;
T_1 ;
    %wait E_0x7fffebfd3c20;
    %load/vec4 v0x7fffebfe4950_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x7fffebfe49f0_0;
    %assign/vec4 v0x7fffebfe4ae0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %pushi/vec4 0, 255, 8;
    %assign/vec4 v0x7fffebfe4ae0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x7fffebf9b030;
T_2 ;
    %delay 50, 0;
    %load/vec4 v0x7fffebfe4f30_0;
    %inv;
    %store/vec4 v0x7fffebfe4f30_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x7fffebf9b030;
T_3 ;
    %pushi/vec4 0, 0, 3;
    %split/vec4 1;
    %assign/vec4 v0x7fffebfe5110_0, 0;
    %split/vec4 1;
    %assign/vec4 v0x7fffebfe5020_0, 0;
    %assign/vec4 v0x7fffebfe4f30_0, 0;
    %pushi/vec4 255, 0, 8;
    %assign/vec4 v0x7fffebfe4c60_0, 0;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x7fffebfe4d40_0, 0;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffebfe5020_0, 0, 1;
    %vpi_call 2 18 "$display", v0x7fffebfe4e10_0 {0 0 0};
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffebfe5020_0, 0, 1;
    %vpi_call 2 20 "$display", v0x7fffebfe4e10_0 {0 0 0};
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fffebfe5110_0, 0, 1;
    %vpi_call 2 22 "$display", v0x7fffebfe4e10_0 {0 0 0};
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fffebfe5110_0, 0, 1;
    %vpi_call 2 24 "$display", v0x7fffebfe4e10_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
